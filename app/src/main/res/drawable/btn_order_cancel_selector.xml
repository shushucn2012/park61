<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="false">
        <shape>
          <corners android:radius="4dip" />
   		  <stroke android:color="@color/g666666" android:width="1dip" />
   		  <solid android:color="@color/gffffff" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="4dip" />
    		<stroke android:color="@color/g999999" android:width="1dip" />
    		<solid android:color="@color/g999999" />
        </shape> 

	</item>
</selector>
